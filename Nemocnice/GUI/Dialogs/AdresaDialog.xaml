<Window x:Class="Nemocnice.GUI.Dialogs.AdresaDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nemocnice.GUI.Dialogs"
        mc:Ignorable="d"
        Title="Adresa Dialog" Height="320
        " Width="400">
    <Grid>
        <StackPanel Margin="10">
            <Label>Cislo Popisne:</Label>
            <TextBox x:Name="CisloPopisneTb" />
            <Label>Ulice:</Label>
            <TextBox x:Name="UliceTb" />
            <Label>Mesto:</Label>
            <TextBox x:Name="MestoTb" />
            <Label>Stat:</Label>
            <TextBox x:Name="StatTb" />
            <Label>PSC:</Label>
            <TextBox x:Name="PSCTb" />
            <StackPanel Orientation="Horizontal" Margin="0,20,0,0">
                <Button Content="OK" IsDefault="True" Click="OK_Click" Width="80" />
                <Button Content="Cancel" IsCancel="True" Click="Cancel_Click" Width="80" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
