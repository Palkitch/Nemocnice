<Window x:Class="Nemocnice.GUI.Catalog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nemocnice.GUI"
        mc:Ignorable="d"
        Title="Katalog" Height="450" Width="800">
    <Grid>
        <ComboBox Name="comboBox" DockPanel.Dock="Top" SelectedIndex="0" SelectionChanged="ComboBox_SelectedIndexChanged" Height="29" VerticalAlignment="Top" VerticalContentAlignment="Center"/>
        <DataGrid Name="dataGridView" AutoGenerateColumns="True" Margin="0,29,0,32" />
        <Button Name="closeButton" Content="Zavřít" DockPanel.Dock="Bottom" Click="CloseButton_Click" Height="32" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,402,0,0" Width="800"/>
    </Grid>
</Window>
